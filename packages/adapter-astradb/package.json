{
  "name": "@auth/astradb-adapter",
  "version": "0.0.1",
  "description": "Astra adapter for next-auth.",
  "homepage": "https://authjs.dev",
  "repository": "https://github.com/nextauthjs/next-auth",
  "bugs": {
    "url": "https://github.com/nextauthjs/next-auth/issues"
  },
  "author": "DataStax",
  "contributors": [
    "Syed Hakeem Abbas <hakeemsyd@gmail.com>"
  ],
  "license": "ISC",
  "keywords": [
    "next-auth",
    "@auth",
    "Auth.js",
    "next.js",
    "oauth",
    "astradb"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js"
    }
  },
  "files": [
    "*.d.ts*",
    "*.js",
    "src"
  ],
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "./tests/test.sh",
    "build": "tsc"
  },
  "dependencies": {
    "@auth/core": "workspace:*",
    "axios": "^1.5.1",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@auth/adapter-test": "workspace:*",
    "@auth/tsconfig": "workspace:*",
    "jest": "^27.4.3"
  },
  "jest": {
    "preset": "@auth/adapter-test/jest",
    "transformIgnorePatterns": [
      "./node_modules/(?!axios)/"
    ]
  }
}