{
  "name": "@auth/nextjs",
  "version": "0.0.0-6f96004d",
  "description": "Authentication for Next.js.",
  "keywords": [
    "authentication",
    "authjs",
    "jwt",
    "nextjs",
    "oauth",
    "oidc",
    "passwordless",
    "react"
  ],
  "homepage": "https://nextjs.authjs.dev",
  "repository": "https://github.com/nextauthjs/next-auth.git",
  "author": "Balázs Orbán <info@balazsorban.com>",
  "scripts": {
    "dev": "tsc -w",
    "clean": "rm -rf *.js *.d.ts lib",
    "build": "pnpm clean && tsc"
  },
  "devDependencies": {
    "@types/react": "18.0.37",
    "typescript": "^4",
    "next": "13.3.1"
  },
  "dependencies": {
    "@auth/core": "workspace:*"
  },
  "peerDependencies": {
    "next": "^13.3.1",
    "react": "^18.2.0"
  },
  "type": "module",
  "types": "./index.d.ts",
  "files": [
    "*.js",
    "*.d.ts",
    "lib",
    "src"
  ],
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js"
    },
    "./client": {
      "types": "./client.d.ts",
      "import": "./client.js"
    },
    "./package.json": "./package.json"
  }
}
